cat > /etc/sysupgrade.conf <<-__EOF__
/etc/shadow
/etc/dropbear/dropbear_dss_host_key
/etc/dropbear/dropbear_rsa_host_key
/etc/dropbear/authorized_keys
/etc/network.config
/etc/config/fff
/etc/config/gateway
/etc/hoodfile
__EOF__

exit 0
